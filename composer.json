{
    "name": "mohsenabrishami/stethoscope",
    "description": "laravel server monitoring",
    "keywords": [
        "stethoscope",
        "server monitoring",
        "laravel"
    ],
    "homepage": "https://github.com/MohsenAbrishami/stethoscope",
    "authors": [
        {
            "name": "Mohsen Abrishami",
            "email": "mohsenabrishami.ma@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.5"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "MohsenAbrishami\\Stethoscope\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MohsenAbrishami\\Stethoscope\\StethoscopeServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^7.11",
        "phpunit/phpunit": "^9.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}